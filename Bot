import telebot
from telebot import types


bot = telebot.TeleBot("1037729395:AAGJLQa4fKdTN-rZkAH3VO7sgDQaaGbwNfQ")

@bot.message_handler(commands=["start"])
def any_msg(message):
    keyboard = types.InlineKeyboardMarkup()
    male_button = types.InlineKeyboardButton(text="Хлопець", callback_data="male")
    female_button = types.InlineKeyboardButton(text="Дівчина", callback_data="female")
    keyboard.add(male_button, female_button)
    bot.send_message(message.chat.id, "Хелло,з ким маю справу \U0001F60F ?", reply_markup=keyboard)
@bot.message_handler(commands=["menu"])
def bars(message):
    keyboard = types.InlineKeyboardMarkup()
    male_button1 = types.InlineKeyboardButton(text="free пари \U0001F35F ", callback_data="free_pairs")
    male_button2 = types.InlineKeyboardButton(text="посиденьки з друзями \U0001F37B", callback_data="with_friends")
    male_button3 = types.InlineKeyboardButton(text="coffee break \U00002615", callback_data="coffee_break")
    male_button4 = types.InlineKeyboardButton(text="хочу почути себе аристократом \U0001F451", callback_data="restaurant")
    keyboard.add(male_button1)
    keyboard.add(male_button2)
    keyboard.add(male_button3)
    keyboard.add(male_button4)
    bot.send_message(message.chat.id, "Ну що ти вже придумав, де будеш тусуватися зі своїми друзями \U0001F4A5?", reply_markup=keyboard)

@bot.callback_query_handler(func=lambda call: True)
def callback_inline(call):
    if call.data == "male":
        keyboard = types.InlineKeyboardMarkup()
        keyboard.row(types.InlineKeyboardButton(text="free пари \U0001F35F", callback_data="free_pairs"))
        keyboard.row(types.InlineKeyboardButton(text="посиденьки з друзями \U0001F37B", callback_data="with_friends"))
        keyboard.row(types.InlineKeyboardButton(text="coffee break \U00002615", callback_data="coffee_break"))
        keyboard.row(types.InlineKeyboardButton(text="хочу почути себе аристократом \U0001F451", callback_data="restaurant"))
        bot.edit_message_text(chat_id=call.message.chat.id, message_id=call.message.message_id,
                              text="Що змусило тебе звернутися до мене, чоловіче \U0001F647?",reply_markup=keyboard)
        # Уведомление в верхней части экрана
        bot.answer_callback_query(callback_query_id=call.id, show_alert=False,
                                  text = "Чим можу, тим поможу, тримайся там \U0000270A")
    elif call.data == "female":
        keyboard = types.InlineKeyboardMarkup()
        keyboard.row(types.InlineKeyboardButton(text="free пари \U0001F369", callback_data="free_pairs"))
        keyboard.row(types.InlineKeyboardButton(text="посиденьки з друзями \U0001F378", callback_data="with_friends"))
        keyboard.row(types.InlineKeyboardButton(text="coffee break \U0001F36E", callback_data="coffee_break"))
        keyboard.row(types.InlineKeyboardButton(text="хочу почути себе аристократкою \U0001F478", callback_data="restaurant"))
        bot.edit_message_text(chat_id=call.message.chat.id, message_id=call.message.message_id, text="Що змусило тебе звернутися до мене, падружка \U0001F646?", reply_markup=keyboard)
        # Уведомление в верхней части экрана
        bot.answer_callback_query(callback_query_id=call.id, show_alert=False, text="Як щодо побачення, красоткa \U0001F490?")

