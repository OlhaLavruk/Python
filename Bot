@bot.message_handler(commands=["geolocation"])
def geophone(message):
    keyboard = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
    button_geo = types.KeyboardButton(text="локатіон, пліс", request_location=True)
    keyboard.add(button_geo)
    bot.send_message(message.chat.id, "Відправ мені геолокацію!", reply_markup=keyboard)

@bot.message_handler(commands=["help"])
def help(message):
    bot.send_message(message.chat.id, "/start - початок роботи бота\n/help - довідка\n/del - видалити дані закінчити роботу\n/geolocation - ми знайдемо тебе швидше, ніж кредитори\n/menu - вибрати для себе ідеальне місце, щоб поїсти")


@bot.message_handler(content_types=["text"])
def any_msgg(message):
    
    bot.send_message(message.chat.id, "Цей бот зробили студенти, думаєш, вони правильно запрограмували мене переписуватися з тобою? я теж так думаю, тому не видумуй і швидше нажимай на /start \U000021A9 \nA якщо все так погано тоді клікай сюди \U000021AA /help, ми звичайно нічим тобі не допоможем ")

if __name__ == "__main__":
    bot.polling(none_stop=True)
